{
  "env": {
    "browser": true,
    "es2021": true
  },
  "globals": {
    "JSX": true
  },
  "extends": [
    "@rocketseat/eslint-config/react",
    "plugin:@next/next/recommended"
  ],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "camelcase": "off"
  }
}